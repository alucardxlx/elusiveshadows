[FUNCTION PORTAL_MAGE]
IF <RESTEST i_portalmage_teleport>
 IF <FINDID.i_portalmage_teleport.TIMER><=-1
  FINDID.i_portalmage_teleport.REMOVE
 ENDIF
ENDIF
IF !<RESTEST i_portalmage_teleport>
 SERV.NEWITEM=i_portalmage_teleport
 NEW.TAG.GO=<ARGV[0]>
 EQUIP <NEW.UID>
ELSE
 SYSMESSAGE @88,,1 You are already Portaling...
ENDIF


[ITEMDEF i_portalmage_teleport]
ID=i_memory
TYPE=t_eq_script
NAME=Portal Item

ON=@EQUIP
 MORE=10
 TIMER=1
 SRC.DIALOGCLOSE d_vendor_buysell_new
 SRC.SAY @88,,1 [Teleporting]
 TAG.X=<CONT.P.X>
 TAG.Y=<CONT.P.Y>
 CONT.EVENTS +e_teleporting_c

ON=@TIMER
 IF <CONT.P.Y>!=<TAG.Y>
  CONT.SAY @88,,1 [You stop teleporting: moved]
  CONT.EVENTS -e_teleporting_c
  REMOVE
  RETURN 1
 ENDIF
 IF <CONT.P.X>!=<TAG.X>
  CONT.SAY @88,,1 [You stop teleporting: moved]
  CONT.EVENTS -e_teleporting_c
  REMOVE
  RETURN 1
 ENDIF
 IF <MORE><=0
  CONT.GO <TAG.GO>
  CONT.EVENTS -e_teleporting_c
  CONT.SYSMESSAGE @88,,1 You have been teleported...
  REMOVE
  RETURN 1
 ELSE
  CONT.SAY @88,,1 [<EVAL <MORE>>]
  MORE=<MORE>+-1
  CONT.EFFECT 3,i_fx_sparkle,30,32,0,55,4
  CONT.SOUND snd_spell_recall
  TIMER=1
 ENDIF
 RETURN 1

[EVENTS e_teleporting_c]

ON=@GETHIT
 IF <RESTEST i_portalmage_teleport>
  SAY @88,,1 [You stop teleporting: You got hit]
  FINDID.i_portalmage_teleport.remove
 ENDIF
 EVENTS -e_teleporting_c

ON=@SKILLSTART
 IF <RESTEST i_portalmage_teleport>
  SAY @88,,1 [You stop teleporting: You started a skill]
  FINDID.i_portalmage_teleport.remove
 ENDIF
 EVENTS -e_teleporting_c



