  DISPID=i_robe
  UPDATEX
  RETURN 1
 ELSEIF <DISPID>==i_robe
  DISPID=i_death_shroud
  UPDATEX
  RETURN 1
 ENDIF

ON=@UNEQUIP
 DISPID=i_robe
 UPDATEX
 RETURN 0


