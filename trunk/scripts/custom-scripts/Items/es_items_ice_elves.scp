[ITEMDEF i_robe_ice_elf]
DEFNAMEi_robe_ice_elf
NAME=icy elven robe
ID=i_robe
ARMOR=30

CATEGORY=NPC Item Packs
SUBSECTION=Ice Elves
DESCRIPTION=Icy Elven Robe

ON=@create
