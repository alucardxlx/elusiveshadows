[ITEMDEF i_ice_cougar_summoning_pot]
DEFNAME=i_ice_cougar_summoning_pot
NAME=pot of near frozen liquid
ID=i_pot_of_wax

CATEGORY=NPC Item Packs
SUBSECTION=Ice Elves
DESCRIPTION=Ice Cougar Summoning Pot

ON=@Create
 MORE=5
 COLOR=047f

ON=@CLICK
 MSG=<NAME> <EVAL <MORE>> Charges

ON=@DCLICK
 IF (<MORE>==0)
  MSG The <NAME> is out of charges.
  REMOVE
  RETURN 1
 ENDIF
 IF <SRC.RESTEST 1 i_summon_timer_ice>
  MSG You cant use this again yet.
  RETURN 1
 ENDIF

 MORE -= 1
 NAME=<NAME> <EVAL <MORE>> Charges
 SRC.NEWITEM=i_summon_timer_ice
 NEW.BOUNCE
 SRC.SMSG You scatter some of the cold, almost frozen, liquid contents on the ground.
 SRC.NEWITEM=i_frozen_contents
 NEW.COLOR=047f
 NEW.P=<SRC.P>
 NEW.ATTR=attr_decay|010
 NEW.TIMER=10
 NEW.LINK=<SRC.UID>
 RETURN 1

[ITEMDEF i_frozen_contents]
NAME=Ice Helper Stuff
ID=01036

CATEGORY=NPC Item Packs
SUBSECTION=Ice Elves
DESCRIPTION=Frozen Contents

ON=@TIMER
 LINK.NEWNPC=c_ice_cougar
 LINK.NEW.P=<P>
 LINK.NEW.FLAGS=04000000
 LINK.TAG.ELE=<LINK.ACT.UID>
 LINK.NEW.UPDATE
 LINK.NEWITEM=i_custom_mem_ele
 LINK.NEW.COLOR memory_ipet
 LINK.NEW.LINK <LINK.UID>
 LINK.NEW.MORE <LINK.UID>
 LINK.NEW.MORE2 memory_ipet
 LINK.NEW.TIMER 300
 LINK.NEW.CONT <EVAL <LINK.TAG.ELE>>
 LINK.MSG An ice cougar spawns. It seems to obey you.
 LINK.MSG You must wait an hour before making another.
 LINK.TAG.ELE=
 LINK.UPDATE
 REMOVE
 RETURN 1


